{
  "compilerOptions": {
    "target": "ES2018",
    "outDir": "./",
    "baseUrl": "./",
    "lib": [],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["**/*.ts",],
  "exclude": ["node_modules"]
}